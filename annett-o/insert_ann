""" Will later be used for dynamic ANNConfig subclass definitions """

from owlready2 import *
from typing import List, Dict
from utils_annett_o import create_subclass

# Load ontology

onto = get_ontology("./ontology/annett-o/annett-o-0.1.owl").load()

base_class = onto.ANNConfiguration

class_names = ["CNN"]

for class_name in class_names:
    new_class = create_subclass(ontology=onto, class_name=class_name, base_class=base_class)

onto.save(file='./ontology/annett-o/annett-o-0.1.owl') # Updated with ANNConfiguration Subclass (CNN)
