from rdflib import Graph

g = Graph()
g.parse("/Users/josueochoa/Desktop/Capstone/Automatic_Taxonomy_Construction_NNs/owl_parsing/annett-o-0.1.owl", format="xml")

jsonld_data = g.serialize(format="json-ld", indent=4)

with open("anneto.jsonld", "w") as f:
    f.write(jsonld_data)

print("OWL saved as JSON-LD")
